{
    "body": {
        "flex": 1,
        "backgroundColor": "#121212"
    },
    "contentContainer": {
        "justifyContent": "center",
        "alignItems": "center"
    },
    "container": {
        "flex": 1,
        "padding": 20,
        "marginTop": 40
    },
    "title": {
        "fontSize": 24,
        "marginBottom": 20,
        "color": "white",
        "fontWeight": "bold",
        "textAlign": "center"
    },
    "row": {
        "flexDirection": "row",
        "marginBottom": 15,
        "justifyContent": "space-around",
        "alignItems": "center",
        "width": "100%"
    },
    "input": {
        "borderWidth": 1,
        "borderColor": "#aaa",
        "padding": 10,
        "width": "90%",
        "marginBottom": 10,
        "borderRadius": 8,
        "color": "white",
        "backgroundColor": "#333"
    },
    "rectBox": {
        "backgroundColor": "#2e2e2e",
        "width": 160,
        "height": 120,
        "borderRadius": 15,
        "marginHorizontal": 5,
        "justifyContent": "center",
        "alignItems": "center"
    },
    "button": {
        "backgroundColor": "#4caf50",
        "width": 140,
        "paddingVertical": 15,
        "paddingHorizontal": 25,
        "borderRadius": 8
    },
    "buttonSquare": {
        "backgroundColor": "#4caf50",
        "width": 70,
        "paddingVertical": 15,
        "paddingHorizontal": 25,
        "borderRadius": 8
    },
    "squareBox": {
        "backgroundColor": "#2e2e2e",
        "width": 100,
        "height": 100,
        "borderRadius": 15,
        "marginHorizontal": 5,
        "justifyContent": "center",
        "alignItems": "center"
    },
    "buttonText": {
        "color": "white",
        "fontSize": 18,
        "fontWeight": "600"
    }
}